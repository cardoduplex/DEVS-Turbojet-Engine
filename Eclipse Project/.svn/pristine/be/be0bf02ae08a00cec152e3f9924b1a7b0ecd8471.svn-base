# Settings.properties

#CalibrationPath=.\xxx\yyy
#CalibrationFile=MyCalibration.json
CalibrationDebug=false

#TypeMappingPath=.\aaa\bbb
#TypeMappingFile=MyTypeMapping.json

#ValueSetPath=.\aaa\bbb
#ValueSetFile=MyValueSet.json

# Set the reference counter for the number of ValueSet consumers for the
# experiment model so that the final user of an instance may remove the
# instance from the ValueSetCache and the garbage collector will be free to
# recover the memory.
ValueSet_Consumers=2

ValueSet_Label=VSet
ValueSet_DebugFormat=%-60s

# Number of concurrent experiment models to run. Range from 1 to 26.
ExperimentModelCount=3
