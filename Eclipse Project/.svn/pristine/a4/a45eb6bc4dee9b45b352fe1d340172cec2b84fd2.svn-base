package turbojet.test;

import turbojet.toolkit.Calibration;

/**
 * The Class TestCalibrationSub1.
 */
public class TestCalibrationSub1 {

  /** The KeABC_Str_Test1. */
  private final String  KeABC_Str_Test1;

  /** The KeABC_Cnt_Test1. */
  private final int     KeABC_Cnt_Test1;

  /** The KeABC_Bool_Test1. */
  private final boolean KeABC_Bool_Test1;

  /**
   * Instantiates a new test calibration sub 1.
   */
  TestCalibrationSub1() {
    this.KeABC_Str_Test1 = "Test 1";
    this.KeABC_Cnt_Test1 = 12345;
    this.KeABC_Bool_Test1 = true;

    final Calibration calibration = new Calibration(false);
    calibration.load();
    calibration.update(this);
  }

  /* (non-Javadoc)
   * @see java.lang.Object#toString()
   */
  @Override
  public String toString() {
    return "Class: " + getClass() + " : " + KeABC_Str_Test1 + " : " +
            KeABC_Cnt_Test1 + " : " + KeABC_Bool_Test1;
  }
}
