package turbojet.toolkit;

import java.util.HashMap;

public class ValueSetCache {

  public HashMap<String, ValueSet> valueSetMap = new HashMap<String, ValueSet>();

  public ValueSetCache() {
  }

  public ValueSet newValueSet(final int experiment, final int counter) {
    final ValueSet valueSet = new ValueSet(experiment, counter);
    valueSetMap.put(valueSet.getKey(), valueSet);
    return valueSet;
  }
}
