package TurbojetEngineMod;

import TurbojetEngineMod.engine.EngineProcessStage;
import experiment.toolkit.ValueSet;

/**
 * The Class EngineStageGeneric.
 */
public class EngineStageGeneric extends EngineBase {

  /** The engine process stage. */
  private EngineProcessStage engineProcessStage;

  /**
   * Instantiates a new engine stage generic.
   */
  public EngineStageGeneric() {
    this("StageGeneric", 'A');
  }

  /**
   * Instantiates a new engine stage generic.
   *
   * @param name the name
   * @param version the version
   */
  public EngineStageGeneric(final String name, final char version) {
    super(name, version);
    engineProcessStage = new EngineProcessStage(name);
  }

  /* (non-Javadoc)
   * @see model.modeling.atomic#initialize()
   */
  @Override
  public void initialize() {
    super.initialize();
  }

  /* (non-Javadoc)
   * @see turbojet.toolkit.ValueSetInterface#processValueSet(turbojet.toolkit.ValueSet)
   */
  @Override
  public void processValueSet(final ValueSet valueSet) {
    engineProcessStage.phase = phase;
    engineProcessStage.sigma = sigma;
    engineProcessStage.clock = clock;
    engineProcessStage.processValueSet(valueSet);
    phase = engineProcessStage.phase;
    sigma = engineProcessStage.sigma;
    clock = engineProcessStage.clock;
  }
}
