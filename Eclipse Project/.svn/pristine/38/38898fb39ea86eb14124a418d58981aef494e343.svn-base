package turbojet.toolkit;

import java.util.HashMap;

public class ValueSet {

  public int experiment;

  public int counter;

  private HashMap<String, Value> valueMap = new HashMap<String, Value>();

  public ValueSet(final int experiment, final int counter) {
    this.experiment = experiment;
    this.counter = counter;
  }

  public String getKey() {
    return "Key" + experiment + "_" + counter;
  }

  public void addValue(final String name, final Object value) {
    valueMap.put(name, new Value(name, value));
  }

  public class Value {

    public String name;

    public Object value;

    public Value(final String name, final Object value) {
      this.name = name;
      this.value = value;
    }
  }
}
