package experiment.model;

import java.util.Queue;

import experiment.toolkit.ValueSet;

/**
 * The Interface ExperimentEngineBaseInterface.
 */
public interface ExperimentEngineFrontPorchInterface {

  /**
   * Start experiments.
   *
   * @param valueSetQueueOut the value set queue out
   */
  public void startExperiments(final Queue<ValueSet> valueSetQueueOut);

  /**
   * Process experiments.
   *
   * @param valueSetQueueIn the value set queue in
   */
  public void processExperiments(final Queue<ValueSet> valueSetQueueIn);

  /**
   * Are experiments complete.
   *
   * @return true, if successful
   */
  public boolean areExperimentsComplete();

  /**
   * Gets the model count.
   *
   * @return the model count
   */
  public int getModelCount();
}
