package experiment.design.util;

import java.util.ArrayList;
import java.util.HashMap;
import java.util.List;
import java.util.Map;

/**
 * The Class DesignExperiment.
 */
public class DesignExperiment {

  /** The factor list. */
  private List<Factor> factorList = new ArrayList<Factor>();

  /** The experiment list. */
  private List<Experiment> experimentList = new ArrayList<Experiment>();

  /**
   * Instantiates a new design experiment.
   */
  public DesignExperiment() {
  }

  /**
   * Gets the factor list.
   *
   * @return the factor list
   */
  public List<Factor> getFactorList() {
    return factorList;
  }

  /**
   * Gets the experiment list.
   *
   * @return the experiment list
   */
  public List<Experiment> getExperimentList() {
    return experimentList;
  }

  /**
   * The Class Factor.
   */
  public class Factor {

    /** The name. */
    public String name;

    /** The value. */
    public String value;

    /** The levels. */
    public Map<String, String> levels = new HashMap<String, String>();
  }

  /**
   * The Class Experiment.
   */
  public class Experiment {

    /** The run. */
    public int run;

    /** The pattern. */
    public String pattern;

    /** The levels. */
    public String levels[];

    /**
     * Instantiates a new experiment.
     *
     * @param run the run
     * @param pattern the pattern
     * @param levelCount the level count
     */
    public Experiment(final int run, final String pattern, final int levelCount) {
      this.run = run;
      this.pattern = pattern;
      levels = new String[levelCount];
    }
  }
}
