package TurbojetEngineMod.engine;

import experiment.toolkit.Calibration;
import experiment.toolkit.ExperimentLog;
import experiment.toolkit.ValueSet;

abstract class EngineStageBase {

  protected String name;

  protected ExperimentLog log;

  protected String group;

  public EngineStageBase(final String name) {
    this.name = name;
    log = new ExperimentLog(super.getClass().getName());
    group = log.getName();
  }

  abstract void process(final ValueSet valueSet, final Calibration calibration);
}
