package experiment.design.util;

/**
 * The Class ExperimentMonitor.
 */
public class ExperimentMonitor {

  /**
   * Instantiates a new experiment monitor.
   */
  public ExperimentMonitor() {
  }

  /**
   * Dump.
   */
  public void dump() {
    dump("");
  }

  /**
   * Dump.
   *
   * @param prefix the prefix
   */
  public void dump(final String prefix) {
    System.out.println(prefix + "Experiment Monitor");
  }
}
