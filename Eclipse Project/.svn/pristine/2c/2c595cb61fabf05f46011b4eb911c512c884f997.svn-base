package experiment.design.util;

/**
 * The Class DesignExperimentRun.
 */
public class DesignExperimentRun {

    /** The run. */
    private int run;

    /** The name. */
    private String name;

    /** The levels. */
    private String levels[];

    /**
     * Instantiates a new design experiment run.
     *
     * @param run the run
     * @param name the name
     * @param levelCount the level count
     */
    public DesignExperimentRun(final int run, final String name, final int levelCount) {
      this.run = run;
      this.name = name;
      levels = new String[levelCount];
    }

  /**
   * Gets the run.
   *
   * @return the run
   */
  public int getRun() {
    return run;
  }

  /**
   * Gets the name.
   *
   * @return the name
   */
  public String getName() {
    return name;
  }

  /**
   * Gets the levels.
   *
   * @return the levels
   */
  public String[] getLevels() {
    return levels;
  }

  /* (non-Javadoc)
   * @see java.lang.Object#toString()
   */
  public String toString() {
    boolean first = true;
    String message = "Run " + run +": " + name + " [";
    for (int i = 0; i < levels.length; i++) {
      if (first)
        first = false;
      else
        message += ", ";
      message += levels[i];
    }
    message += "]";
    return message;
  }
}
